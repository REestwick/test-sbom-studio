#!/usr/bin/env groovy

pipeline {
    agent any
    stages {
        stage('Stage 1') {
            steps {
                echo 'Testing Jenkins SBOM Studio Plugin'
                sh 'pwd'
                sh 'ls -al'
            }
        }
        stage('Stage 2') {
            steps {
                echo 'Hello world!'
            }
        }
    }
}